{
  "compilerOptions": {
    "outDir": "dist-electron/scripts/",
    "declaration": false,
    "skipLibCheck": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "target": "ESNext",
    "allowSyntheticDefaultImports": true,
    "rootDir": "electron/scripts"
  },
  "types": ["node"],
  "include": ["electron/scripts/*"]
}
